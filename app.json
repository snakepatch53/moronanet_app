{
    "expo": {
        "name": "Morona Net",
        "slug": "moronanet_app",
        "version": "1.0.2",
        "orientation": "portrait",
        "icon": "./assets/app_icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "moronanet"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/app_icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.snakepatch53.moronanet_app"
        },
        "web": {
            "favicon": "./assets/app_icon.png"
        },
        "extra": {
            "eas": {
                "projectId": "5c8390af-345a-4d2c-8d4f-3f1d31c1e254"
            }
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ]
        ]
    }
}
